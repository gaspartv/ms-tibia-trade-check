{
  "name": "api-tibia-trade-check",
  "version": "1.0.0",
  "description": "API Fastify para scraping de dados do Tibia.com",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "tibia",
    "scraper",
    "fastify",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.10.9",
    "cheerio": "^1.1.2",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "fastify": "^5.6.2",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/node": "^25.0.3",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
